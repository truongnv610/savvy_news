<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTabsNavigator">
        <attr name="toolbarColor" format="reference"/>
        <attr name="secondaryToolbarColor" format="reference"/>
        <attr name="showTitle" format="boolean"/>
        <attr name="browserStartAnimation" format="reference"/>
        <attr name="activityExitAnimation" format="reference"/>
        <attr name="browserExitAnimation" format="reference"/>
        <attr name="activityEnterAnimation" format="reference"/>
    </declare-styleable>

    <declare-styleable name="InfoMapBubbleView">
        <attr name="backgroundBubbleColor" format="color"/>
        <attr name="strokeColor" format="color"/>
        <attr name="strokeWidth" format="dimension"/>
        <attr name="cornerRadius" format="dimension"/>
        <attr name="android:paddingStart"/>
        <attr name="android:paddingTop"/>
        <attr name="android:paddingEnd"/>
        <attr name="android:paddingBottom"/>
        <attr name="android:padding"/>
    </declare-styleable>

    <declare-styleable name="CardLayout">
        <attr name="card_borderWidth" format="dimension"/>
        <attr name="card_cornerRadius" format="dimension"/>
        <attr name="card_semiCircleRadius" format="dimension"/>
        <attr name="card_backgroundColor" format="color|reference"/>
        <attr name="card_borderColor" format="color|reference"/>
        <attr name="card_cornerDirection" format="enum">
            <enum name="none" value="0x00"/>
            <enum name="topLeft" value="0x01"/>
            <enum name="topRight" value="0x02"/>
            <enum name="bottomLeft" value="0x04"/>
            <enum name="bottomRight" value="0x08"/>
            <enum name="top" value="0x03"/>
            <enum name="left" value="0x05"/>
            <enum name="right" value="0x0A"/>
            <enum name="bottom" value="0xC"/>
            <enum name="all" value="0x0F"/>
        </attr>
        <attr name="card_semiCircleDirection" format="enum">
            <enum name="none" value="0x00"/>
            <enum name="left" value="0x01"/>
            <enum name="top" value="0x02"/>
            <enum name="right" value="0x04"/>
            <enum name="bottom" value="0x08"/>
            <enum name="horizontal" value="0x05"/>
            <enum name="vertical" value="0x0A"/>
            <enum name="all" value="0x0F"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ProductQuantityView">
        <attr name="productQuantity_minValue" format="integer"/>
        <attr name="productQuantity_maxValue" format="integer"/>
        <attr name="productQuantity_value" format="integer"/>
    </declare-styleable>

    <declare-styleable name="OtpTextView">
        <attr name="android:textColor" />
        <attr name="otp" format="string" />
        <attr name="hide_otp" format="boolean" />
        <attr name="hide_otp_drawable" format="reference" />
        <attr name="length" format="integer" />
        <attr name="width" format="dimension" />
        <attr name="height" format="dimension" />
        <attr name="box_margin" format="dimension" />
        <attr name="box_margin_left" format="dimension" />
        <attr name="box_margin_right" format="dimension" />
        <attr name="box_margin_top" format="dimension" />
        <attr name="box_margin_bottom" format="dimension" />
        <attr name="bar_height" format="dimension" />
        <attr name="bar_enabled" format="boolean" />
        <attr name="otp_box_background" format="reference" />
        <attr name="otp_box_background_active" format="reference" />
        <attr name="otp_box_background_inactive" format="reference" />
        <attr name="otp_box_background_success" format="reference" />
        <attr name="otp_box_background_error" format="reference" />
        <attr name="bar_margin" format="dimension" />
        <attr name="bar_margin_bottom" format="dimension" />
        <attr name="bar_margin_right" format="dimension" />
        <attr name="bar_margin_left" format="dimension" />
        <attr name="bar_margin_top" format="dimension" />
        <attr name="otp_text_size" format="dimension" />
        <attr name="bar_active_color" format="color" />
        <attr name="bar_inactive_color" format="color" />
        <attr name="bar_error_color" format="color" />
        <attr name="bar_success_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ProfileDetailListItem">
        <attr name="leadingIcon" format="reference" />
        <attr name="textLabel" format="string" />
    </declare-styleable>
</resources>